struct Rect
    x: Float
    y: Float
    w: Float
    h: Float

fn area(rect: Rect *) : Float
    return rect->w * rect->h

fn main(): Int
    var sqr: Rect
    
    while a lt b:
        a = a + c
        c = c + 1
    
    sqr.x = 0
    sqr.y = 0
    scan sqr.w
    scan sqr.h

    print area(&sqr)
    print '\n'

    return 0
